[Unit]
Description=Backend Server
After=network.target

[Service]
ExecStart=/home/pi/sofia-plotter/bin/python server.py
WorkingDirectory=/home/pi/actions-runner/_work/sofia-plotter/sofia-plotter/web-apps/svg-dropper/backend
Restart=on-failure
RestartSec=5
User=pi

[Install]
WantedBy=multi-user.target